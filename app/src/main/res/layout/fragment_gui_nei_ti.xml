            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@mipmap/weixin"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text=""
